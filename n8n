#!/bin/bash
# n8n Manager CLI - Global Command Wrapper (Linux/macOS)
# This script allows you to run 'n8n' from anywhere

# Get the directory where this script is located
SCRIPT_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

# Run the Python script from the core folder
python3 "$SCRIPT_DIR/core/n8n_manager.py" "$@"
